package com.te.assignment.assignment02;

import java.util.ArrayList;

public class EmployeeTest {
	public static void main(String[] args) {
		ArrayList<Employee> arrayList = new ArrayList<Employee>();
		arrayList.add(new Employee(10, 1, "Deepankar", 4200.56));
		arrayList.add(new Employee(30, 4, "Sankar", 9880.56));
		arrayList.add(new Employee(50, 2, "Bhupati", 5400.56));
		arrayList.add(new Employee(20, 3, "Anil", 3200.56));
		arrayList.add(new Employee(40, 5, "Anil2", 4000.56));
		
		System.out.println(arrayList);
	}
}
